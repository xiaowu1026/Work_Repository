            <xml-property name="queryText"><![CDATA[select l.description,
       (SELECT a.description
          FROM csh_transaction_classes_vl a
         WHERE a.csh_transaction_type_code = 'PREPAYMENT'
           AND a.enabled_flag = 'Y'
           AND a.csh_transaction_class_code = l.csh_transaction_class_code) cls_des,
       (select fc.company_short_name
          from fnd_companies_vl fc
         where fc.company_id = l.company_id) company_name,
       to_char(l.amount, 'FM999,999,999,990.00') requisition_amount,
       decode(l.partner_category,
              'VENDER',
              (select psv.DESCRIPTION
                 from pur_system_venders_vl psv
                where psv.vender_id = l.partner_id),
              'EMPLOYEE',
              (select e.name
                 from exp_employees e
                where e.employee_id = l.partner_id),
              'CUSTOMER',
              (select osc.DESCRIPTION
                 from ord_system_customers_vl osc
                where osc.CUSTOMER_ID = l.partner_id)) payee_name,
       l.account_name,
       l.account_number,
       l.bank_name
  from csh_payment_requisition_lns l
 where l.payment_requisition_header_id = ?
]]></xml-property>
