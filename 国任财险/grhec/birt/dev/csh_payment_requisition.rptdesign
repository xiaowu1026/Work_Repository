            <xml-property name="queryText"><![CDATA[select h.payment_requisition_header_id,
       h.requisition_number,
       (select e.name
          from exp_employees e
         where e.employee_id = h.employee_id) employee_name,
       (select scv.code_value_name
          from sys_code_values_v scv
         where scv.code = 'PAYMENT_REQUISITION_STATUS'
           and scv.code_value = h.status) status_desc,
       to_char(h.requisition_date, 'YYYY-MM-DD') requisition_date,
       h.attachment_num,
       (select u.description
          from exp_org_unit_vl u
         where u.unit_id = h.unit_id) unit_name,
       (select gc.currency_name
          from gld_currency_vl gc
         where gc.currency_code = h.currency_code) currency_name,
       to_char(h.amount, 'FM999,999,999,990.00') requisition_amount,
       h.description
  from csh_payment_requisition_hds h
 where 1 = 1
]]></xml-property>
