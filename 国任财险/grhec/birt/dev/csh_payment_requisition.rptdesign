            <xml-property name="queryText"><![CDATA[select t.instance_id,
       t.document_number,
       to_char(t1.creation_date, 'YYYY-MM-DD') as creation_date,
       t1.user_name,
       t1.action_title,
       t1.comment_text,
       n.name
  from wfl_workflow_instance t,
       wfl_approve_record_v  t1,
       wfl_workflow          w,
       wfl_workflow_node_vl  n
 where t.instance_id = t1.instance_id
   and t1.node_id = n.node_id(+)
   and t.instance_id =
       (select max(i.instance_id)
          from wfl_workflow_instance i, wfl_workflow w1
         where i.document_number = t.document_number
           and i.workflow_id = w1.workflow_id
           and w1.workflow_category = 'PAYMENT_REQUISITION')
   and t1.workflow_id = w.workflow_id
   and w.workflow_category = 'PAYMENT_REQUISITION'
   and t.instance_param = ?
 order by creation_date
]]></xml-property>
