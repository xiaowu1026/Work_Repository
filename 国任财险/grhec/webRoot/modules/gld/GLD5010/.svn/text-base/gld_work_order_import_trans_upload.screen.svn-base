<a:screen xmlns:a="http://www.aurora-framework.org/application" trace="true">
    <a:init-procedure>
        <a:model-query model="gld.GLD5010.gld_work_order_import_status" rootPath="status"/>
        <a:model-query model="gld.gld_work_order_get_sys_import_head_id" rootPath="header"/>
        <a:import-excel header_id="${/model/header/record/@header_id}" separator="," status_field="/parameter/@ImportSuccess" template_code="GLDIMPORT" user_id="${/session/@user_id}"/>
    </a:init-procedure>
    <a:view>
        <a:link id="gld_work_order_import_link" url="${/request/@context_path}/modules/gld/GLD5010/gld_work_order_import.screen"/>
        <a:link id="gld_work_order_import_save_data_link" url="${/request/@context_path}/modules/gld/GLD5010/gld_work_order_import_save_data.svc"/>
        <script><![CDATA[
            function loadComplete() {
                
                    var param = {};
                    param['batch_id'] = '${/parameter/@batch_id}';
                    param['header_id'] = '${/model/header/record/@header_id}';
                    param['work_order_header_id'] = '${/parameter/@work_order_header_id}';
                        Aurora.request({
                            url: 
                            $('gld_work_order_import_save_data_link').getUrl(),
                            para: param,
                            success: function() {
                                Ext.get('success_form').setStyle('visibility', 'visible');
                            },
                            scope: this
                        }); 
                }
            
            function view() {
                window.location.href = 
                $('gld_work_order_import_link').getUrl() + '?work_order_header_id=${/parameter/@work_order_header_id}' + '&batch_id=${/parameter/@batch_id}' + '&work_order_type_id=' + '${/parameter/@work_order_type_id}';
                window.close();
            }
            
            function back() {
                window.location.href = 
                $('gld_work_order_import_link').getUrl() + '?work_order_header_id=${/parameter/@work_order_header_id}' + '&batch_id=${/parameter/@batch_id}' + '&work_order_type_id=' + '${/parameter/@work_order_type_id}';
            }
        ]]></script>
