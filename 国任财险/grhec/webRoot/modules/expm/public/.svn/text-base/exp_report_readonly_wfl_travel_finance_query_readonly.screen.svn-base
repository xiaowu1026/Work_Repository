              function summaryRenderer(datas, name) {
                var sum = 0;
                for (var i = 0;i < datas.length;i++) {
                    var r = datas[i];
                    var d = r.get(name);
                    var n = parseFloat(d);
                    if (!isNaN(n)) {
                        sum += n;
                    }
                }
                var total = (typeof(sum) == 'undefined' ? '' : parseFloat(sum).toFixed(2));
                return '<font>'  + Convert(total) + '</font>';
            }
             function Convert(value) {
                if (value) {
                    var s = value;
                    s += "";
                    if (s.indexOf(".") == -1) {
                        s += ".00";
                    }
                    if (/\.\d$/.test(s)) {
                        s += "0";
                    } //正则判断
                    while (/\d{4}(\.|,)/.test(s)) {
                        s = s.replace(/(\d)(\d{3}(\.|,))/, "$1,$2");
                    }
                    return s;
                } else {
                    return '';
                }
            }
            
