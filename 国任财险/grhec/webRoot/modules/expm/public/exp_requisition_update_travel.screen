                if ($('exp_requisition_head_ds').getAt(0).get('budget_control_enabled') == 'Y') {
                    Aurora.request({
                        url: /*${/request/@context_path}/autocrud/expm.exp_requisition_bgt_check/update*/
                        $('exp_requisition_bgt_check_link').getUrl(),
                        para: {
