            function lineupdate(dataSet, record, name, value, oldvalue) {
                if (name == 'usage_type_desc') {
                    if (value != oldvalue) {
                        show_msg = '您已修改费用用途，请删除凭证后，重新创建凭证!';
                    } else {
                        show_msg = '';
                    }
                }
                 //费用用途是视同销售，视同销售税率和视同销售税额必输
                if(name=="usage_type"){
                    if(record.get('usage_type').indexOf('AS_SALES')>=0){
                        record.getField('special_sales_tax_rate_desc').setReadOnly(false);
                        record.getField('special_sales_tax_rate_desc').setRequired(true);
                        record.getField('special_sales_tax_amount').setRequired(true);
                    }else{
                        record.getField('special_sales_tax_rate_desc').setReadOnly(true);
                        record.getField('special_sales_tax_rate_desc').setRequired(false);
                        record.getField('special_sales_tax_amount').setRequired(false);
                        record.set('special_sales_tax_rate_desc','');
                        record.set('special_sales_tax_rate','');
                        record.set('special_sales_tax_amount','');
                    }
                }
                if(name=='special_sales_tax_rate_desc'){
                    var tax_rate_desc=record.get('special_sales_tax_rate_desc');
                    if(tax_rate_desc !=null){
                    var index=tax_rate_desc.indexOf('%');
                    var tax_rate=tax_rate_desc.substring(0,index)/100;//税率
                    record.set('special_sales_tax_rate',tax_rate);
                    //视同销售税额=报销不含税金额*视同销售税率
                    var special_slaes_tax_amount=record.get('sale_amount')*tax_rate;
                    record.set('special_sales_tax_amount',special_slaes_tax_amount);
	                    if (value != oldvalue) {
	                        show_msg = '用于视同销售，请删除凭证后，重新创建凭证!';
	                    } else {
	                        show_msg = '';
	                    }
                    }
                }
            }
            
             function summaryRenderer(datas, name) {
                var sum = 0;
                for (var i = 0;i < datas.length;i++) {
                    var r = datas[i];
                    var d = r.get(name);
                    var n = parseFloat(d);
                    if (!isNaN(n)) {
                        sum += n;
                    }
                }
                var total = (typeof(sum) == 'undefined' ? '' : parseFloat(sum).toFixed(2));
                return '<font>'  + Convert(total) + '</font>';
            }
             function Convert(value) {
                if (value) {
                    var s = value;
                    s += "";
                    if (s.indexOf(".") == -1) {
                        s += ".00";
                    }
                    if (/\.\d$/.test(s)) {
                        s += "0";
                    } //正则判断
                    while (/\d{4}(\.|,)/.test(s)) {
                        s = s.replace(/(\d)(\d{3}(\.|,))/, "$1,$2");
                    }
                    return s;
                } else {
                    return '';
                }
            }
            
