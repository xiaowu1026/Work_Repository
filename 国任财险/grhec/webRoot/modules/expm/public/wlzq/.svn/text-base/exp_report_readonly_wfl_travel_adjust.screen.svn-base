            function delete_adjust(){
                if ($('adjust_result').getSelected().length == 0) {
                    Aurora.showInfoMessage('${l:PROMPT}', '${l:PROMPT.SELECT_DATA}', null, 250, 100);
                    return;
                }
                var okCan = Aurora.showConfirm('${l:PROMPT}', '${l:DELETE_DATA_CONFIRM}', function() {
                    var ds = $('adjust_result');
                    var rs = ds.getSelected();
                    for (var i = 0;i < rs.length;i++) {
                        if (rs[i].isNew) {
                            ds.remove(rs[i]);
                            i--;
                        }
                    }
                    var param = $('adjust_result').getJsonData(true);
                    if (param.length && param.length != 0) {
                        var url = '';
                        for (var i = 0;i < param.length;i++) {
                            param[i]['_status'] = 'delete';
                        }
            
                        url = $('adjust_result_delete_link').getUrl();
                        
                        Aurora.request({
                            url: url,
                            para: param,
                            success: function() {
                                var rs = ds.getSelected();
                                for (var i = rs.length - 1;i >= 0;i--) {
                                    rs[i].isNew = true;
                                    ds.remove(rs[i]);
                                }
                                $('adjust_result').query();
                            },
                            scope: this
                        });
                        okCan.close();
                    } else {}
                }, null, 250, 100);
            }
            
