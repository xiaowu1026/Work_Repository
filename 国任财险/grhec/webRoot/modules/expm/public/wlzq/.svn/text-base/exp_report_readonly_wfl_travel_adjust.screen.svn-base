        <script><![CDATA[
            function init() {
                var t = $('exp_report_head').getAt(0); 
                var r = t.getMeta();
                var f1 = r.getField('exp_report_rate_type_name');
                var f2 = r.getField('exp_report_rate_quotation_name');
                var f3 = r.getField('exp_report_rate');
                //是否与发票金额相符
                r.getField('consistent_invoice_amount').setReadOnly(true);
                if ("Y" == t.get('consistent_invoice_amount')) {
                    $('grid').showColumn('invoice_amount');
                    $('grid').showColumn('invoice_tax_amount');
                } else {
                    $('grid').hideColumn('invoice_amount');
                    $('grid').hideColumn('invoice_tax_amount');
                }
                //end add
                if (t.get('exp_report_currency_code') != t.get('functional_currency_code')) {
                    f1.setRequired(true);
                    f2.setRequired(true);
                } else {
                    f1.setReadOnly(true);
                    f2.setReadOnly(true);
                    f3.setReadOnly(true);
                    t.set('exp_report_rate', 1);
                }
                if (t.get('exp_report_payee_category') == 'EMPLOYEE') {
                    if ('${/parameter/@exp_report_header_id}' == '') {
                        t.set('exp_report_payee_id', '${/parameter/@employee_id}');
                        t.set('exp_report_payee_name', t.get('exp_report_employee_name'));
                    }
                }
                if ('${/parameter/@exp_report_header_id}' != '') {
                    r.getField('exp_report_data').setReadOnly(true);
                    r.getField('exp_report_payment_method').setReadOnly(true);
                    r.getField('exp_report_position').setReadOnly(true);
                    r.getField('exp_report_payee_category_name').setReadOnly(true);
                    r.getField('exp_report_payee_name').setReadOnly(true);
                    f1 = r.getField('exp_report_payee_name');
                    var payee_category = t.get('exp_report_payee_category_name');
                    if (payee_category == 'EMPLOYEE') {
                        f1.setLovService('expm.exp_req_employee_lov');
                    } else if (payee_category == 'VENDER') {
                        f1.setLovService('fnd.pur_system_venders');
                    } else {
                        f1.setLovService('fnd.ord_system_customers');
                    }
                }
                var hidename = '';
                if ('${/model/policy_enabled/record/@policy_enabled}' == 'Y') {
                    hidename = 'city';
                } else {
                    hidename = 'place';
                }
                var columns = $('grid').columns;
                for (var i = 0,
                    l = columns.length;i < l;i++) {
                    var colname = columns[i].name;
                    if (colname == hidename) {
                        $('grid').hideColumn(colname);
                    }
                }
            }
            init();
            
            new function() {
                var check_count = '${/model/check_log_count/record/@check_count}';
                var att_count = '${/model/att_flag/record/@attachments}';
                if (check_count != 0) {
                    document.getElementById('excess_exspense_standard').style.display="block";
                    $('seeLog').el.setStyle('color', 'red');
                }
                if (att_count != 0) {
                    $('uploadBtn').el.setStyle('color', 'red');
                }
                if ('${/model/check_policy_count/record/@check_count}' > 0) 
                {
                     document.getElementById('excess_exspense_standard').style.display="block";
                     $('seeLog').el.setStyle('color', 'red');
                } else {
                    $('seeLog').el.setStyle('color', '');
                }
            }();
        ]]></script>
        <script purpose="auto_resize"><![CDATA[
                function expRepROWFLStandardInitSize() {
                    //描述宽度
                    var labelWidth = 75;
                    //标签宽度,5 = 3padding + 2border-spacing
                    var tagWidth = 150 + 5;
                    //页面宽度、高度
                    var vw = $A.getViewportWidth();
                    //留白宽度
                    var marginWidth = 35;
                    //自适应宽度
                    var autoWidth = vw - marginWidth;
                    //Form内部宽度，-2border
                    var formInnerWidth = autoWidth - 2;
                    //所占列数
                    var colSpan = 4;
                    //设置组件的自适应宽度
                    Ext.get('exp_rep_ro_wfl_standard_head_form').setWidth(autoWidth + 4);
                    $('line_tab').setWidth(autoWidth + 4);
                    $('pmt_line_tab').setWidth(autoWidth + 4);
                    $('adjust_line_tab').setWidth(autoWidth + 4);
                    $('grid').setWidth(autoWidth+4-17);
                    $('pmt_grid').setWidth(autoWidth+4-17);
                    //$('pmt_grid').setWidth(autoWidth + 4 - 17);
                     $('journal_adjust_result_grid').setWidth(autoWidth+4-17);
                    //设置TextArea的自适应宽度,别问我最后那个6是哪里来的，试出来的
                    Ext.get('exp_rep_ro_wfl_standard_desc_ta').setWidth(formInnerWidth - (formInnerWidth / colSpan - labelWidth - tagWidth) - labelWidth - 6);
                }
                //Ext.fly(window).on('resize', expRepROWFLStandardInitSize);
                expRepROWFLStandardInitSize();
            ]]></script>
