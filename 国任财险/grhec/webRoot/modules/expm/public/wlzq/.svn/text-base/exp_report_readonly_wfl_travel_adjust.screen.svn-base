                // } else if (vat_special_invoice_include == "N") {
                    // //报销单按现有的正常显示。
                    // $('grid').hideColumn('invoice_type_desc');
                    // $('grid').hideColumn('invoice_number');
                    // $('grid').hideColumn('invoice_status_desc');
                    // $('grid').hideColumn('invoice_code');
                    // $('grid').hideColumn('invoice_date');
                    // $('grid').hideColumn('usage_type_desc');
                    // $('grid').hideColumn('tax_amount');
                    // $('grid').hideColumn('sale_amount');
                    // $('grid').hideColumn('tax_rate');
                    // $('grid').hideColumn('input_tax_struc_detail_desc');
                // }
            	
            	//行加载
                 var records=$('exp_report_lines').getAll();
                 for(var i=0;i<records.length;i++){
                       var record=records[i];
                       if (record.get('invoice_type') == 'INV001' || record.get('special_invoice') == 'Y') { //增值税专用发票
                        $('vat_usage_type_ds').setQueryParameter('type_code',null);
                        $('vat_usage_type_ds').query();
                    }
                    if (record.get('invoice_type') == 'INV002') { //电子普通发票
                        $('vat_usage_type_ds').setQueryParameter('type_code', '09-AS_SALES');
                        $('vat_usage_type_ds').query();
            
                    }
                    if (record.get('invoice_type') == 'INV003') { //增值税普通发票
                        $('vat_usage_type_ds').setQueryParameter('type_code', '09-AS_SALES');
                        $('vat_usage_type_ds').query();
                    }
                      //通用机打发票及其他
                    if (record.get('invoice_type') == 'INV099') {
                        $('vat_usage_type_ds').setQueryParameter('type_code', '09-AS_SALES');
                        $('vat_usage_type_ds').query();
                    }
                    
                   if(record.get('usage_type')!=null){
	                     if(record.get('usage_type').indexOf('AS_SALES')>=0){
	                        record.getField('special_sales_tax_rate_desc').setReadOnly(false);
	                        record.getField('special_sales_tax_rate_desc').setRequired(true);
	                        record.getField('special_sales_tax_amount').setRequired(true);
	                    }else{
	                        record.getField('special_sales_tax_rate_desc').setReadOnly(true);
	                        record.getField('special_sales_tax_rate_desc').setRequired(false);
	                        record.getField('special_sales_tax_amount').setRequired(false);
	                    }
                   }
                 }
            	
                //隐藏关联批扣流水,关联收入流水
                var ref_pay_flag = '${/model/budget_control/record/@ref_pay_flag}';
                var ref_gater_flag = '${/model/budget_control/record/@ref_gather_flag}';
                var expense_report_type_code = '${/model/budget_control/record/@expense_report_type_code}';
                if (!ref_pay_flag || ref_pay_flag != 'Y') {
                    //$('grid').hideColumn('ref_pay_num');
                }
                if (!ref_gater_flag || ref_gater_flag != 'Y') {
                    //$('grid').hideColumn('ref_gather_num');
                }
            
                if (expense_report_type_code == '20') {
                    $('grid').showColumn('price');
                    $('grid').showColumn('primary_quantity');
                } else {
                    $('grid').hideColumn('price');
                    $('grid').hideColumn('primary_quantity');
                }
            
            }
            
