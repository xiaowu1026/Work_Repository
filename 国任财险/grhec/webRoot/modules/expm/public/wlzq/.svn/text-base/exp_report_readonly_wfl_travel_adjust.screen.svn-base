            function savePmtFunc() {
                if (!$('exp_report_lines').validate()) {
                    return;
                }
                if (!$('pmtplan_schedules_ds').validate()) {
                    return;
                }
                
                //提醒
                if (show_msg) {
                    Aurora.showInfoMessage('${l:PROMPT}', show_msg, function() {
                        show_msg = '';
                        $('exp_report_head').submit();
                    }, 250, 100);
                } else {
                    $('exp_report_head').submit();
                }
                // $('exp_report_lines').submit();
                // $('pmtplan_schedules_ds').submit();
            }
            
