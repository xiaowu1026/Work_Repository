            function savereport() {
                //modified by zyh 20180516
                var head_ds = $('exp_report_head');
                var je_creation_status = head_ds.getAt(0).get('je_creation_status');
                var exp_report_header_id = head_ds.getAt(0).get('exp_report_header_id');
                var d1 = $('exp_report_head_dimension');
                var d2 = $('exp_report_head_object');
                var dimension = d1.getAt(0).data;
                var dynamic_id = '';
                for (var p in dimension) {
                    if (p.substring(0, 1) == 'e') {
                        dynamic_id = dimension[p];
                    }
                }
                if (!$('pmtplan_schedules_ds').validate()) {
                    return;
                }
                if (!$('exp_report_head').validate()) {
                    return;
                }
                var param = {};
                //修改头上支付公司字段，其余字段取原表值
                param['exp_report_header_id'] = exp_report_header_id;
                param['exp_report_barcode'] = head_ds.getAt(0).get('exp_report_barcode');
                param['employee_id'] = head_ds.getAt(0).get('employee_id');
                param['payee_category'] = head_ds.getAt(0).get('exp_report_payee_category');
                param['payee_id'] = head_ds.getAt(0).get('exp_report_payee_id');
                param['exp_report_type_id'] = head_ds.getAt(0).get('exp_report_type_id');
                param['expense_user_group_id'] = head_ds.getAt(0).get('expense_user_group_id');
                param['report_date'] = head_ds.getAt(0).get('exp_report_data');
                param['period_name'] = head_ds.getAt(0).get('period_name');
                param['report_status'] = head_ds.getAt(0).get('report_status');
                param['gld_interface_flag'] = head_ds.getAt(0).get('gld_interface_flag');
                param['attachment_num'] = head_ds.getAt(0).get('exp_report_attach_id');
                param['description'] = head_ds.getAt(0).get('exp_report_description');
                param['reversed_flag'] = head_ds.getAt(0).get('reversed_flag');
                param['source_exp_rep_header_id'] = head_ds.getAt(0).get('source_exp_rep_header_id');
                param['source_type'] = head_ds.getAt(0).get('source_type');
                param['payment_method_id'] = head_ds.getAt(0).get('payment_method_id');
                param['vat_special_invoice_include'] = head_ds.getAt(0).get('vat_special_invoice_include');
                param['consistent_invoice_amount'] = head_ds.getAt(0).get('consistent_invoice_amount');
                param['pay_company_id'] = head_ds.getAt(0).get('pay_company_id');
                Aurora.request({
                    url: $('update_header_dynamic_link').getUrl() + '?dynamic_id=' + dynamic_id,
                    para: param,
                    success: function() {},
                    scope: this
                });
            }
            
            function pmtGridcellclick(grid, row, name, record) {
                var metadata = record.getMeta();
                if (name == 'cash_flow_item_description') {
                    var default_code = record.get('usedes');
                    metadata.getField('cash_flow_item_description').setLovService('csh.CSH1075.csh_default_flowitems_lov');
                    metadata.getField('cash_flow_item_description').setLovPara('default_type', 'PAYMENT_USEDES');
                    metadata.getField('cash_flow_item_description').setLovPara('default_code', default_code);
                }
            
            }
            
            function pmtplanOnLoad(ds) {
                var pmtplan_schedules_records = ds.getAll();
                // var pmtplan_schedules_records = $('pmtplan_schedules_ds').getAll();
                for (i = 0;i < pmtplan_schedules_records.length;i++) {
                    //付款用途代码
                    var usedes_code = pmtplan_schedules_records[i].get('usedes');
                    var cash_flow_item_description = pmtplan_schedules_records[i].get('cash_flow_item_description');
                    // 默认计划付款行的现金流量项；
                    var csh_default_flowitems_records = $('cash_flow_item_description_ds').getAll();
                    var pmtplan_record = pmtplan_schedules_records[i];
                    if (cash_flow_item_description == '' || cash_flow_item_description == null) {
            
                        for (j = 0;j < csh_default_flowitems_records.length;j++) {
                            var default_type = csh_default_flowitems_records[j].get('default_type');
                            var default_code = csh_default_flowitems_records[j].get('default_code');
                            var cash_flow_item_id = csh_default_flowitems_records[j].get('cash_flow_item_id');
                            var cash_flow_item_description = csh_default_flowitems_records[j].get('cash_flow_item_description');
                            if (default_code == usedes_code) {
                                pmtplan_record.set('cash_flow_code', cash_flow_item_id);
                                pmtplan_record.set('default_code', default_code);
                                pmtplan_record.set('cash_flow_item_description', cash_flow_item_description);
                            }
                        }
                    }
                }
            }
            
