            function show_write_off_message() {
            
                var pmt_records = $('pmtplan_schedules_ds').getAll();
                var wirte_off_flag = '';
                for (var i = 0;i < pmt_records.length;i++) {
                    var un_write_off_flag = pmt_records[i].get('un_write_off_flag');
                    if (un_write_off_flag == 'N') {
                        wirte_off_flag = 'Y';
                        break;
                    } else {
                        wirte_off_flag = 'N';
                    }
                }
            
                if (wirte_off_flag == 'Y') {
                    Aurora.showInfoMessage('${l:PROMPT}', '该报销单存在可核销的借款，请查看计划付款行处是否核销借款,后审批!');
