                var autoWidth = vw - marginWidth;
                Ext.get('t_csh_form').setWidth(autoWidth+6);
                $('line_tab').setWidth(autoWidth+8);
                $('t_csh_grid').setWidth(autoWidth);
                $('grid_1').setWidth(autoWidth);
            }
