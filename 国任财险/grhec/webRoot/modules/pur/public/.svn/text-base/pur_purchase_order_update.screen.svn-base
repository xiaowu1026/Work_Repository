            //提交
            
            function submitfun() {
                savefunction(submitsuccess);
                // if (changedFlag) {
                // Aurora.showInfoMessage('${l:PROMPT}', '${l:PROMPT.SAVE_SUBMIT}', null, 250, 100);
                // } else {
                // submitsuccess();
                // }
