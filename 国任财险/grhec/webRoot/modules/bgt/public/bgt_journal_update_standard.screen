            function line_cellClick(grid, row, name, record) {
                //[39751]预算日记账录入时预算期间需校验是否关闭
            
                if (name == 'company_id') {
                    record.getMeta().getField('line_period_name').setLovPara('p_company_id', record.get('company_id'));
                }
                if (name == 'responsibility_center_name') {
                    record.getMeta().getField('responsibility_center_name').setLovPara('p_company_id', record.get('company_id'));
                    record.getMeta().getField('responsibility_center_name').setLovPara('p_position_id', '${/model/bgt_header_info/record/@position_id}');
                }
                if (name == 'description') {
                    record.getMeta().getField('description').setLovPara('p_company_id', record.get('company_id'));
                }
                if (name.substr(0, 1) == 'd') {
                    var dimension_sequence = name.replace('d', '');
                    if (dimension_sequence <= 11 && record.get('company_id')) {
                        record.getMeta().getField(name).setLovService('bgt.bgt_journal_dimension_value_lov');
                        record.getMeta().getField(name).setLovPara('company_id', record.get('company_id'));
                        record.getMeta().getField(name).setLovPara('dimension_sequence', dimension_sequence);
                    } else {}
                    if (dimension_sequence == 12 && record.get('company_id')) {
                        record.getMeta().getField(name).setLovService('bgt.bgt_journal_line_dim_e12_lov');
                        record.getMeta().getField(name).setLovPara('E11', record.get('company_id'));
                    }
                    if (dimension_sequence == 13 && record.get('company_id')) {
                        record.getMeta().getField(name).setLovService('bgt.bgt_journal_line_dim_e13_lov');
                        record.getMeta().getField(name).setLovPara('E11', record.get('company_id'));
                        if (record.get('e12')) {
                            record.getMeta().getField(name).setLovPara('E12', record.get('e12'));
                        }
                    }
                    if (dimension_sequence == 14 && record.get('company_id')) {
                        record.getMeta().getField(name).setLovService('bgt.bgt_journal_line_dim_e14_lov');
                        record.getMeta().getField(name).setLovPara('E11', record.get('company_id'));
                        if (record.get('e12')) {
                            record.getMeta().getField(name).setLovPara('E12', record.get('e12'));
                        }
                        if (record.get('e13')) {
                            record.getMeta().getField(name).setLovPara('E13', record.get('e13'));
                        }
                    }
                    if (dimension_sequence == 15 && record.get('company_id')) {
                        record.getMeta().getField(name).setLovService('bgt.bgt_journal_line_dim_e15_lov');
                        record.getMeta().getField(name).setLovPara('E11', record.get('company_id'));
                        if (record.get('e12')) {
                            record.getMeta().getField(name).setLovPara('E12', record.get('e12'));
                        }
                        if (record.get('e13')) {
                            record.getMeta().getField(name).setLovPara('E13', record.get('e13'));
                        }
                        if (record.get('e14')) {
                            record.getMeta().getField(name).setLovPara('E14', record.get('e14'));
                        }
                    }
                }
            }
            
