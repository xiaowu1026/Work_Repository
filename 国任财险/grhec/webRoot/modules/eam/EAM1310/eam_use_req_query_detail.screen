                    <a:column name="to_address" align="center" prompt="物理地址" width="120"/>
                    <!-- <a:column name="product_desc" align="center" prompt="产品" width="140"/>
                    <a:column name="channel_desc" align="center" prompt="渠道" width="140"/> -->
