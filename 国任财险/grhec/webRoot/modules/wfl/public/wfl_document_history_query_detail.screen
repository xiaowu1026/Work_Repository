             /**---------------------以下函数不需要改动--------------------------------**/
            
            function listFetchByKey(list, key, attribs) {
                var map = {},
                    result = [];
                for (var i = 0;i < list.length;i++) {
                    var data = list[i].data || list[i];
                    var val = data[key];
                    var lt = map[val];
                    if (!lt) {
                        lt = [];
                    }
                    lt.push(data);
                    map[val] = lt;
                }
                for (var k in map) {
                    var record = {};
                    record[key] = k;
                    if (map[k].length > 0) for (var j = 0;j < attribs.length;j++) {
                        var att = attribs[j];
                        record[att] = map[k][0][att]
                    }
                    var cd = map[k];
                    for (var m = 0;m < cd.length;m++) {
                        delete cd[m][key];
                        for (var j = 0;j < attribs.length;j++) {
                            var att = attribs[j];
                            delete cd[m][att];
                        }
                    }
            
                    record['children'] = cd;
                    result.push(record);
                }
                for (var i = 0;i < result.length;i++) {
                    var data = result[i];
                    var n = {
                        count: 0
                    };
                    countChildren(n, data.children);
                    if (n['count'] != 0) {
                        data[key + '_count'] = n['count'];
                        for (var j = 0;j < attribs.length;j++) {
                            var att = attribs[j];
                            data[att + '_count'] = n['count'];
                        }
                    }
                }
                return result;
            }
            
            var rowNumber = 1;
            
            function extendAttribute(o, c) {
                if (o && c && typeof c == 'object') {
                    for (var p in c) {
                        if (p == 'children') continue;
                        o[p] = c[p];
                    }
                }
                return o;
            };
            
            function processDatas(list, result) {
                for (var i = 0;i < list.length;i++) {
                    var record = list[i];
                    if (!record['_line']) {
                        record['_line'] = rowNumber;
                    }
                    result.push(record);
                    if (record && record.children) {
                        processDatas(record.children, result);
                    } else {
                        rowNumber++;
                    }
                }
            }
            
            function countChildren(map, children) {
                if (!children) return;
                for (var i = 0;i < children.length;i++) {
                    var child = children[i];
                    if (child.children) {
                        countChildren(map, child.children)
                    } else {
                        var count = map['count'];
                        if (!count) {
                            count = 0;
                        }
                        count++;
                        map['count'] = count;
                    }
                }
            }
            
