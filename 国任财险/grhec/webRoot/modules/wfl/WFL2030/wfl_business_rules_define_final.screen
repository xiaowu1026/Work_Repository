            </a:dataSet>
            <a:dataSet id="wfl_business_rule_detail_result_ds" autoCount="true" autoQuery="true" fetchAll="true" model="wfl.WFL2030.wfl_business_rule_final" queryUrl="${/request/@context_path}/autocrud/wfl.WFL2030.wfl_business_rule_final/query?business_rule_id=${/parameter/@business_rule_id}" selectable="true">
