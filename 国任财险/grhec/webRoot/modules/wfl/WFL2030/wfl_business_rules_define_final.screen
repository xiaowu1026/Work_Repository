            <a:grid id="business_rule_detail_result" bindTarget="wfl_business_rule_detail_result_ds" marginHeight="170" navBar="false">
                <a:columns>
                    <a:column name="line_number" prompt="WORKFLOW_RULE.RULE_SEQUENCE" width="50"/>
