#container {
	overflow: hidden;
	margin: 0 auto;
	width: 988px;
}

.title {
	overflow: hidden;
	border: 0;
	height: 38px;
	margin: 0;
	border-bottom:1px solid #cecece;
}

.title label {
	display: inline-block;
	font: bold 16px/36px "Î¢ÈíÑÅºÚ";
	padding: 0 2px 0 8px;
	width: auto;
	color:#f98818;
}

.help-title {
	overflow: hidden;
	border: 0;
	height: 38px;
	margin: 0;
	border-bottom:1px solid #cecece;
	background:url(../images/help-title.jpg) no-repeat;
}
.help-title span {
    line-height: 38px;
    margin-left: 50px;
    font-size: 16px;
    color: #fff;
    font-weight: bold;
}
.mainContent {
	border: 1px solid #cecece;
	width: 722px;
	margin-top: 6px;
}

.philanSide {
	width: 248px;
	padding-right: 4px;
}
.workflow {
	width: 216px;
	height: 450px;
	border: 1px solid #cecece;
	margin-top: 6px;
	margin-bottom: 6px;
	background: #fff;
	border: 1px solid #cecece;
}
.consult {
	width: 216px;
	height: 208px;
	border: 1px solid #cecece;
	margin-top: 6px;
	margin-bottom: 6px;
	background: #fff;
	border: 1px solid #cecece;

}

.help {
	width: 216px;
	height: 205px;
	border: 1px solid #cecece;
	margin-top: 6px;
	margin-bottom: 6px;
	background: #fff;
}

.help-border {
	border-left:1px solid #cecece;
	border-right:1px solid #cecece;
	border-bottom:1px solid #cecece;
}

.personInfoSide {
	background: none repeat scroll 0 0 #FFFFFF;
	margin-bottom: 9px;
}

.personInfoSide dl {
	padding: 7px 0 5px 8px;
	overflow: hidden;
	margin-bottom: -5px;
}

.personInfoSide dl dt {
	width: 73px;
	float: left;
	padding-top: 3px;
}

.personInfoSide .mainNav {
