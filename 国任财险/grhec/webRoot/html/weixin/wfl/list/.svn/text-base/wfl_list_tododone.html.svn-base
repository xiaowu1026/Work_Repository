                .handle-loading-tip {
                    width: 100%;
                    height: 100%;
                    display: table;

                }

                .handle-loading-tip span {
                    display: table-cell;
                    vertical-align: middle;
                    text-align: center;
                }

                i.ion-close {
                    color: #c4151c;
                }

                i.ion-checkmark {
                    color: #c4151c;
                }

                i.ion-android-share {
                    color: #c4151c;
                }

                i.ion-android-more-horizontal {
                    color: #c4151c;
                }

                span.t {
                    color: #7c7d7f;
                }

                .tabs-positive > .tabs, .tabs.tabs-positive {
                    border-color: white;
                    background-color: white;
                    background-image: linear-gradient(0deg, #0c63ee, #0c63ee 50%, transparent 50%);
                    color: #fff;
                }

                .br-top-1, .br-top-2 {
                    position: relative !important;
                    left: -6rem !important;
                    width: 6rem !important;
                    background-color: white;
                    text-align: center;
                }

                .select-operation.notify {
                    background-color: #c4151c;
                }

                span.t {
                    background-color: white;
                }

                .select-operation.notify {
                    background-color: #c4151c;
                }

                button.button.ng-binding.button-positive {
                    background-color: #c4151c;
                    border: 1px solid #c4151c;
                }

                /*button.button.ng-binding.button-default {*/
                /*background-color: #c4151c;*/
                /*border:1px solid #c4151c;*/
                /*}*/
            </style>
