<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm" xmlns:f="aurora.database.features" alias="t1">
    <bm:operations>
        <bm:operation name="update">
            <bm:parameters>
                <bm:parameter name="pur_po_ref_types_id"/>
                <bm:parameter name="order_type_id"/>
                <bm:parameter name="expense_type_id"/>
            </bm:parameters>
            <bm:update-sql><![CDATA[
                begin
                 pur_purchase_order_types_pkg.update_pru_po_ref_object_type(${@pur_po_ref_object_types_id},
                                                                           ${/parameter/@order_type_id},
                                                                           ${@expense_object_type_id},
                                                                           ${@layout_position},
                                                                           ${@layout_priority},
                                                                           ${@default_object_id},
                                                                           ${/session/@user_id},
                                                                           nvl(${@required_flag},'n'));  
                end;
            ]]></bm:update-sql>
        </bm:operation>
    </bm:operations>
    <bm:features>
        <f:standard-who/>
    </bm:features>
</bm:model>
