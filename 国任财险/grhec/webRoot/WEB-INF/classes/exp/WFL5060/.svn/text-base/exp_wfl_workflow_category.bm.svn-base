<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[SELECT V.CODE_VALUE, V.CODE_VALUE_NAME
					  FROM SYS_CODE_VALUES_V V
					 WHERE V.CODE = 'WFL_WORKFLOW_CATEGORY'
					 ORDER BY V.CODE_VALUE, V.CODE_VALUE_NAME
					 ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
    <bm:fields>
        <bm:field name="code_value" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="CODE_VALUE"/>
        <bm:field name="code_value_name" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="CODE_VALUE_NAME"/>
    </bm:fields>
</bm:model>
