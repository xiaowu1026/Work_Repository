                        UPPER(T.account_segment2) = FRC.RESPONSIBILITY_CENTER_code(+) AND
                        T.account_segment1        = FC.COMPANY_CODE(+) AND
                        FC.company_id             = FRC.company_id AND
                        T.EXP_REPORT_HEADER_ID     =${@header_id} AND
                        ${@header_id}             IS NOT NULL
                        AND frc.company_id = fc.company_id
                    ) t2
                ORDER BY
                    PERIOD_NAME,
                    EXP_REPORT_DISTS_ID,
                    COMPANY_ID,
                    EXP_REPORT_JE_LINE_ID
            ]]></bm:query-sql>
            <bm:parameters>
                <bm:parameter name="header_id"/>
            </bm:parameters>
        </bm:operation>
        <bm:operation name="delete">
            <bm:update-sql><![CDATA[
                begin
                 exp_report_pkg.wfl_delete_exp_report_accounts(p_exp_report_header_id =>${@exp_report_header_id},
				                                          	p_exp_report_je_line_id =>${@exp_report_je_line_id},
				                                       		p_user_id => ${/session/@user_id});
                end;
            ]]></bm:update-sql>
        </bm:operation>
    </bm:operations>
    <bm:count-sql><![CDATA[
    	
		  select count(1) from exp_report_accounts t
		   where t.exp_report_header_id =${@header_id}
		  and ${@header_id} is not null	
    	
    ]]></bm:count-sql>
</bm:model>
