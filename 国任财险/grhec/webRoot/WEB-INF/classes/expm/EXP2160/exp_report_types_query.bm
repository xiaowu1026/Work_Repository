<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
             	
             	select 'GLD_WORK_ORDER' expense_report_type_code,
             		'核算工单'  description
             	from dual
             	where ${@include_gl_order} = 'Y'
             	
             	union all
                SELECT
                    v.expense_report_type_code,
                    v.description
                FROM
                    exp_sob_report_types_vl v #WHERE_CLAUSE#
                ORDER BY
                    expense_report_type_code
                    
                    
                
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
    <bm:fields>
        <bm:field name="expense_report_type_code" databaseType="VARCHAR2" datatype="java.lang.String" forDisplay="true" forQuery="true" physicalName="EXPENSE_REPORT_TYPE_CODE" prompt="报销单类型代码"/>
        <bm:field name="description" databaseType="VARCHAR2" datatype="java.lang.String" displayWidth="200" forDisplay="true" forQuery="true" physicalName="DESCRIPTION" prompt="报销单类型"/>
    </bm:fields>
    <bm:data-filters>
        <bm:data-filter expression="v.set_of_books_id = (SELECT set_of_books_id FROM fnd_companies fc WHERE fc.company_id = ${/session/@company_id} ) AND v.enabled_flag = &apos;Y&apos;"/>
    </bm:data-filters>
    <bm:query-fields>
        <bm:query-field field="expense_report_type_code" queryOperator="like"/>
        <bm:query-field field="description" queryOperator="like"/>
    </bm:query-fields>
</bm:model>
