            <bm:update-sql><![CDATA[ begin
			exp_report_pkg.create_expense_report_acc_rt(${@exp_report_line_id},
														--'$ {@ batch_id}',
														 to_date(${@journal_date},'yyyy-mm-dd'),
														 ${/session/@company_id},
														 ${/session/@user_id});
                                                    									  
                                 
		end;]]></bm:update-sql>
