<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
             select t.user_id,
					t.user_name,
					t.user_name || ' - ' || t.description NAME
			   from sys_user t
			  where t.user_id = ${/session/@user_id}
				]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
