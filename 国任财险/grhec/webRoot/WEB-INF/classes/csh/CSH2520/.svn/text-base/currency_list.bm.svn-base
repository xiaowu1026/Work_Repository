<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
            SELECT CV.CURRENCY_CODE,
				          CV.CURRENCY_NAME
			  FROM GLD_CURRENCY_VL CV
			 WHERE CV.ENABLED_FLAG = 'Y'
			 ORDER BY CV.CURRENCY_CODE
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
