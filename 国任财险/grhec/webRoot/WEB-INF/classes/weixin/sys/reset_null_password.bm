<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm" needAccessControl="false">
    <bm:operations>
        <bm:operation name="execute">
            <bm:update-sql><![CDATA[
                begin
                    SYS_MOBILE_PKG.reset_null_password(
                                p_user_id     => ${@user_id}
                    );
                end;]]></bm:update-sql>
            <bm:parameters>
                <bm:parameter name="user_id"/>
            </bm:parameters>
        </bm:operation>
    </bm:operations>
</bm:model>
