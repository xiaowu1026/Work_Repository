<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
            
            	select decode(wf.workflow_category,
		              'ACP_INVOICE',
		              'ACP_INVOICE_HEADERS',
		              'ACP_REQUISITION',
		              'ACP_ACP_REQUISITION_HDS',
		              'BANK_TREASURER_REQ',
		              '',
		              'BUDGET_JOURNAL',
		              'BGT_JOURNAL_HEADERS',
		              'CASH_PLAN',
		              '',
		              'CON_CONTRACT',
		              'CON_CONTRACT_HEADERS',
		              'EXP_REPORT',
		              'EXP_REPORT_HEADERS',
		              'EXP_REQUISITION',
		              'EXP_REQUISITION_HEADERS',
		              'PAYMENT_REQUISITION',
		              'CSH_PAYMENT_REQUISITION_HDS',
		              'PUR_ORDER',
		              'PUR_ORDER_HEADERS',
		              'PUR_REQUISITION',
		              'PUR_REQUISITION_HEADERS',
		              '') as doc_header_table,
		       i.instance_param as doc_header_id
		  from wfl_workflow wf, wfl_workflow_instance i
		 where wf.workflow_id = i.workflow_id
		   and i.instance_id = ${@instance_id}
		            	
            
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
