<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="insert">
            <bm:update-sql><![CDATA[
                DECLARE
                    v_invoice_type  VARCHAR2(20);
                    v_price         NUMBER;
                    v_sale_amount   NUMBER;
                    v_currency_code VARCHAR2(50);
                BEGIN
                    SELECT
                        h.currency_code
                    INTO
                        v_currency_code
                    FROM
                        exp_report_headers h
                    WHERE
                        h.exp_report_header_id = ${@exp_report_header_id};
                    SELECT
                        ROUND(to_number(${@report_amount}) / to_number(${@primary_quantity}),
                        (SELECT
                            gcv.PRECISION
                        FROM
                            GLD_CURRENCY_VL gcv
                        WHERE
                            gcv.CURRENCY_CODE = 'CNY'
                        ))
                    INTO
                        v_price
                    FROM
                        dual;
                    v_sale_amount:=to_number(${@report_amount})-to_number(NVL(${@tax_amount},0));
                    exp_report_pkg.save_exp_rep_lines_intf(${@exp_report_header_id},
                                         ${@line_number},
                                         ${@city},
                                         ${@description},
                                         to_date(${@date_from},
                                                 'yyyy-mm-dd  hh24:mi:ss'),
                                         to_date(${@date_to},
                                                 'yyyy-mm-dd  hh24:mi:ss'),
                                         nvl(${@period_name},
                                         bgt_common_pkg.get_bgt_period_name(${@company_id},
                                                                            sysdate)),
                                         nvl(v_currency_code,'CNY'),
                                         ${@exchange_rate_type},
                                         ${@exchange_rate_quotation},
                                         NVL(${@exchange_rate}, 1),
                                         ${@expense_type_id},
                                         ${@exp_req_item_id},
                                         NULL,
                                         ${@price},
                                         ${@primary_quantity},
                                         NULL,
                                         NULL,
                                         NULL,
                                         ${@report_amount},
                                         ${@report_amount},
                                         NULL,
                                         ${@company_id},
                                         ${@unit_id},
                                         ${@position_id},
                                         ${@responsibility_center_id},
                                         ${@employee_id},
                                         ${@payee_category},
                                         ${@payee_id},
                                         NULL,
                                         ${@payment_flag},
                                         'GENERATE',
                                         'N',
                                         NULL,
                                         NULL,
                                         ${@e1},
                                         ${@e2},
                                         ${@e3},
                                         ${@e4},
                                         ${@e5},
                                         ${@e6},
                                         ${@e7},
                                         ${@e8},
                                         ${@e9},
                                         ${@e10},
                                         ${@account_name},
                                         ${@account_number},
                                         ${@payment_type_id},
                                         ${@payment_type},
                                         ${@user_id},
                                         ${@exp_report_line_id},
                                         ${@place_type_id},
                                         ${@place_id},
                                         ${@tax_type_id},
                                         ${@user_id},
                                         ${@contract_header_id},
                                         ${@payment_schedule_line_id},
                                         ${@transportation},
                                         ${@place_to_id},
                                         ${@invoice_type},
                                         ${@special_invoice},
                                         ${@invoice_number},
                                         ${@invoice_status},
                                         ${@tax_amount},
                                         v_sale_amount,
                                         ${@tax_rate},
                                         ${@usage_type},
                                         ${@invoice_code},
                                         to_date(${@invoice_date},
                                                 'yyyy-mm-dd'),
                                         ${@input_tax_structure_detail},
                                         to_date(${@income_period_from},
                                                 'yyyy-mm-dd'),
                                         to_date(${@income_period_to},
                                                 'yyyy-mm-dd'),
                                         '',
                                         '',
                                         '',
                                         ${@invoice_sum_amount},
                                         ${@invoice_tax_amount_bak},
                                         ${@person_number},
                                         ${@day_number},
                                         round(${@report_amount}/(${@person_number}*${@day_number}),2),
                                         ${@employee_levels_id},
                                         ${@invoice_check_code},
                                         ${@special_situation},
                                         ${@invoice_sales_amount});
                END;
            ]]></bm:update-sql>
            <bm:parameters>
                <bm:parameter name="exp_report_header_id" inputPath="@exp_report_header_id"/>
                <bm:parameter name="exp_report_line_id" input="true" inputPath="@exp_report_line_id" output="true" outputPath="@exp_report_line_id"/>
            </bm:parameters>
        </bm:operation>
    </bm:operations>
</bm:model>
