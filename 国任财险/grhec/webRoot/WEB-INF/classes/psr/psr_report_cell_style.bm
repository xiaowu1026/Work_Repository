<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
                SELECT
                    s.cell_style_id,
                    s.align,
                    s.name,
                    s.dataformat,
                    s.pattern,
                    s.bordertop,
                    s.borderright,
                    s.borderleft,
                    s.borderbottom,
                    s.topbordercolor,
                    s.bottombordercolor,
                    s.leftbordercolor,
                    s.rightbordercolor,
                    s.backgroundcolor,
                    s.foregroundcolor
                FROM
                    rpt_report_cell_style s
                WHERE
                    s.report_header_id=${@report_header_id}
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
