<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="insert">
            <bm:query-sql><![CDATA[
                BEGIN
                    vat_input_certificates_pkg.insert_error(p_header_id =>${@header_id}, p_batch_id =>${@batch_id}, p_user_id =>${/session/@user_id});
                END;
            ]]></bm:query-sql>
        </bm:operation>
        <bm:operation name="delete">
            <bm:query-sql><![CDATA[
                BEGIN
                    vat_input_certificates_pkg.delete_error(p_batch_id =>${@batch_id}, p_user_id =>${/session/@user_id});
                END;
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
