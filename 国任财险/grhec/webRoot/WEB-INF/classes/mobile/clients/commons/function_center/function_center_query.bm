<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
select *
  from (select '其他功能' as text,
               ' ' as image_url,
               ' ' as url,
               'SECTION' as function_type,
               '1' as order_field
          from dual
        ) t
 order by t.order_field
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
