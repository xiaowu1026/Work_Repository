<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="update">
            <bm:parameters>
                <bm:parameter name="id" dataType="java.lang.Long" input="true" output="false"/>
                <bm:parameter name="header_id" dataType="java.lang.Long" input="true" output="false"/>
                <bm:parameter inputPath="/session/@user_id"/>
            </bm:parameters>
            <bm:update-sql><![CDATA[
                begin
                    CSH_BONDS_TRANSFER_INFO_PKG.COMFRIM_CSH_BONDS_TRANSFER
                    (
                        p_id=>${@id},
                        p_header_id=>${@payment_requisition_header_id},
                        p_user_id=>${/session/@user_id}
                    );
                end;]]></bm:update-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
