<model xmlns="http://www.aurora-framework.org/schema/bm">
    <operations>
        <operation name="update">
            <update-sql><![CDATA[
                begin
                          sys_parameter_pkg.update_parameter_values(${@parameter_id},
                                                                    ${@module_id},
                                                                    ${@level_id},
                                                                    ${@level_value},
                                                                    ${@parameter_value},
                                                                    ${@value_code},
                                                                    ${@value_name},
                                                                    ${/session/@user_id});
                        end;
            ]]></update-sql>
        </operation>
    </operations>
</model>
