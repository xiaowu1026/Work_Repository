<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="insert">
            <bm:update-sql><![CDATA[ 
            begin
		      exp_sob_report_types_pkg.insert_exp_sob_user_groups(${@expense_report_type_id},
