<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="insert">
            <bm:update-sql><![CDATA[   
				begin
				  csh_cash_plan_pkg.ins_csh_cash_plan_lines(${@cash_plan_line_id},
				                                            ${@cash_plan_header_id},
				                                            ${@line_number},
				                                            ${@cash_plan_item_id},
				                                            to_date(${@date_from},
				                                                    'yyyy-mm-dd'),
				                                            to_date(${@date_to},
				                                                    'yyyy-mm-dd'),
				                                            ${@currency},
				                                            ${@exchange_rate_type},
				                                            ${@exchange_rate_quotation},
				                                            ${@exchange_rate},
				                                            ${@orientation},
				                                            ${@increment_amount},
				                                            ${/session/@company_id},
				                                            ${@operation_unit_id},
				                                            ${@responsibility_center_id},
				                                            ${@unit_id},
				                                            ${@partner_category_id},
				                                            ${@partner_id},
				                                            ${@project_id},
				                                            ${@line_notes},
				                                            ${/session/@user_id});
				end;
			]]></bm:update-sql>
        </bm:operation>
        <bm:operation name="update">
            <bm:update-sql><![CDATA[   
				begin
				  csh_cash_plan_pkg.upd_csh_cash_plan_lines(${@cash_plan_line_id},
				                                            ${@cash_plan_header_id},
				                                            ${@line_number},
				                                            ${@cash_plan_item_id},
				                                            to_date(${@date_from},
				                                                    'yyyy-mm-dd'),
				                                            to_date(${@date_to},
				                                                    'yyyy-mm-dd'),
				                                            ${@currency},
				                                            ${@exchange_rate_type},
				                                            ${@exchange_rate_quotation},
				                                            ${@exchange_rate},
				                                            ${@orientation},
				                                            ${/session/@company_id},
				                                            ${@operation_unit_id},
				                                            ${@responsibility_center_id},
				                                            ${@unit_id},
				                                            ${@partner_category_id},
				                                            ${@partner_id},
				                                            ${@project_id},
				                                            ${@line_notes},
				                                            ${/session/@user_id});
				end;
			]]></bm:update-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
