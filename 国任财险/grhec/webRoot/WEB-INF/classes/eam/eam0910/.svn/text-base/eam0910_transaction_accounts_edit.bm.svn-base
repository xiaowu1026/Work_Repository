    			end;
    		]]></ns1:update-sql>
        </ns1:operation>
        <ns1:operation name="delete">
            <ns1:update-sql><![CDATA[
    			begin
    				eam_requisition_pkg.delete_eam_trx_account_manual(p_transaction_je_line_id => ${@transaction_je_line_id},
							                                  		  p_user_id                => ${/session/@user_id});
