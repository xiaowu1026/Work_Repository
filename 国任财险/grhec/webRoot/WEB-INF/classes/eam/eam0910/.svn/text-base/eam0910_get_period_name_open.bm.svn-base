    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
				  select GLD_COMMON_PKG.get_gld_period_name(p_company_id => ${/session/@company_id}, p_date => to_date(${@journal_date},'yyyy-mm-dd')) as period_name from dual
  			]]></bm:query-sql>
