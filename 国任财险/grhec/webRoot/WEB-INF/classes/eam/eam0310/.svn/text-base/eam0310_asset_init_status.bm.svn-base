<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
            
                SELECT
                    v.code_value_name status_desc
                FROM
                    sys_code_values_v v
                WHERE
                    v.code       = 'EAM_ASSET_STATUS' AND
                    v.code_value = 'NEW'
                    
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
