<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm" xmlns:f="aurora.database.features" alias="t1" baseTable="PRJ_PROJECT_TYPE_ITEMS">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
            	select *
				  from (select t1.prj_type_item_id,
				               t1.prj_type_id,
				               t1.prj_type_item_code,
				               t1.description_id,
				               t1.enabled_flag,
				               (select description_text
				                  from fnd_descriptions
				                 where description_id = t1.description_id
				                   and language = ${/session/@lang}) as description
				          from prj_project_type_items t1
				         where t1.prj_type_id = ${@prj_type_id}
				         order by t1.prj_type_item_code) t
				  #WHERE_CLAUSE#
			]]></bm:query-sql>
        </bm:operation>
        <bm:operation name="insert">
            <bm:update-sql><![CDATA[
			    begin
					prj_project_types_pkg.insert_prj_project_type_items(${@prj_type_item_id},
									                                    ${@prj_type_id},
									                                    ${@prj_type_item_code},
									                                    ${@description},
									                                    nvl(${@enabled_flag},'Y'),
									                                    ${/session/@user_id});
				end;			
			]]></bm:update-sql>
            <bm:parameters>
                <bm:parameter name="prj_type_item_id" output="true" outputPath="@prj_type_item_id"/>
            </bm:parameters>
        </bm:operation>
        <bm:operation name="update">
            <bm:update-sql><![CDATA[
				begin
					prj_project_types_pkg.update_prj_project_type_items(${@prj_type_item_id},
									                                    ${@prj_type_id},
									                                    ${@description},
									                                    nvl(${@enabled_flag},'Y'),
									                                    ${/session/@user_id});
				                                     
				end;			
			]]></bm:update-sql>
        </bm:operation>
    </bm:operations>
    <bm:fields>
        <bm:field name="prj_type_item_id" databaseType="NUMBER" datatype="java.lang.Long" physicalName="PRJ_TYPE_ITEM_ID" prompt="PRJ_PROJECT_TYPE_ITEMS.PRJ_TYPE_ITEM_ID"/>
        <bm:field name="prj_type_id" databaseType="NUMBER" datatype="java.lang.Long" physicalName="PRJ_TYPE_ID" prompt="PRJ_PROJECT_TYPE_ITEMS.PRJ_TYPE_ID"/>
        <bm:field name="prj_type_item_code" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="PRJ_TYPE_ITEM_CODE" prompt="PRJ_PROJECT_TYPE_ITEMS.PRJ_TYPE_ITEM_CODE"/>
        <bm:field name="description_id" databaseType="NUMBER" datatype="java.lang.Long" physicalName="DESCRIPTION_ID" prompt="PRJ_PROJECT_TYPE_ITEMS.DESCRIPTION_ID"/>
        <bm:field name="enabled_flag" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="ENABLE_FLAG" prompt="PRJ_PROJECT_TYPE_ITEMS.ENABLED_FLAG"/>
        <bm:field name="description" databaseType="VARCHAR" datatype="java.lang.String" prompt="PRJ_PROJECT_TYPE_ITEMS.DESCRIPTION"/>
    </bm:fields>
    <bm:features>
        <f:standard-who/>
    </bm:features>
    <bm:primary-key>
        <bm:pk-field name="prj_type_item_id"/>
    </bm:primary-key>
    <bm:query-fields>
        <bm:query-field field="prj_type_item_code" queryExpression="t.prj_type_item_code like ${@prj_type_item_code}||&apos;%&apos;"/>
        <bm:query-field name="description" queryExpression="t.description like ${@description}||&apos;%&apos;"/>
    </bm:query-fields>
</bm:model>
