<?xml version="1.0" encoding="UTF-8"?>
<ns1:model xmlns:ns1="http://www.aurora-framework.org/schema/bm">
    <ns1:operations>
        <ns1:operation name="update">
            <ns1:update-sql><![CDATA[
                begin
                    sys_login_pkg.role_select(
                        ${/cookie/@JSID/@value},
                        ${/model/role/record/@role_id},
                        ${/model/role/record/@company_id},
                        null,
                        ${/request/@address},
                        null);
                end; 
            ]]></ns1:update-sql>
        </ns1:operation>
    </ns1:operations>
</ns1:model>
